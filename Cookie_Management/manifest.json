

  {
    "manifest_version": 2,
    "name": "Cookie Manager",
    "version": "1.0",
    "description": "Manage third-party cookies and block ads.",
    "permissions": [
        "cookies",
        "storage",
        "notifications",
        "webRequest",
        "webRequestBlocking"],
              
      
      "background": {
        "service_worker": "background.js",
        "scripts": ["adBlocker.js"]
      },
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "images/icon16.png",
          "48": "images/icon48.png",
          "128": "images/icon128.png"
        }
      },
      "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  
  